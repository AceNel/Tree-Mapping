{{#partial "content"}}

    <table class="table table-striped" style="background-color:#98ff98">
        <thead>
        <tr>
            <th scope="col"></th>
            <th scope="col">username</th>
            <th scope="col">clan name</th>
            <th scope="col">view details</th>
        </tr>
        </thead>
        <tbody>
        {{#each users}}
            <tr>
                <th scope="row">1</th>
                <td>{{username}}</td>
                <td>{{clan_name}}</td>
                <td><a href="/users/{{id}}"><i class="fa fa-eye text-info"></i></a></td>
            </tr>
        {{/each}}

        </tbody>
    </table>

{{/partial}}
{{>layout.hbs}}